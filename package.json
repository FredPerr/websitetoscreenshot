{
  "name": "websitetovisual",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint:fix": "pnpm lint --fix",
    "test": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test-dev": "start-server-and-test dev http://127.0.0.1:3000 test",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "packageManager": "pnpm@8.6.1",
  "dependencies":{
   "@types/react-dom": "18.2.6",
    "framer-motion": "^10.12.17",
    "next": "13.4.7",
    "npm-run-all": "^4.1.5",
    "postcss": "8.4.24",
    "puppeteer": "^20.7.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "start-server-and-test": "^2.0.0",
    "superjson": "^1.12.3",
    "typescript": "5.1.3",
    "vite": "^4.3.9",
    "zod": "^3.21.4"
  },
  "devDependencies": {
      "daisyui": "^3.1.6",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@playwright/test": "^1.35.1",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@typescript-eslint/parser": "^5.60.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.43.0",
    "eslint-config-next": "^12.3.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "tailwindcss": "3.3.2",
    "tsx": "^3.12.7",
    "vitest": "^0.32.2"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
